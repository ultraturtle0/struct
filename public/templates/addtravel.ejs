<p>{{ distance }}</p>
<form id="travel" name="travel" ng-submit="travelSubmit()" method="post">
	<table>
		<tbody>
			<tr>
				<td>Job Start: </td>
				<td><select ng-options="job as job.JOB_NUMBER for job in jobs" name="JOB_ID_START" ng-model="select.JOB_ID_START"></select></td>
				<td>Location Start: </td>
				<td><input type="text" name="LOC_START" ng-model="select.LOC_START"></td>
				<td ng-show="(travel.JOB_ID_START.$touched || travel.LOC_START.$touched) && (!select.JOB_ID_START && !select.LOC_START)">Choose a job or enter a location.</td>
			</tr>
			<tr>
				<td>Job End: </td>
				<td><select ng-options="job as job.JOB_NUMBER for job in jobs" name="JOB_ID_END" ng-model="select.JOB_ID_END"></select></td>
				<td>Location End: </td>
				<td><input type="text" name="LOC_END" ng-model="select.LOC_END"></td>
				<td ng-show="(travel.JOB_ID_END.$touched || travel.LOC_END.$touched) && (!select.JOB_ID_END && !select.LOC_END)">Choose a job or enter a location.</td>
			</tr>
			<tr>
				<td>Employee: </td>
				<td><select ng-options="emp._id as emp.EMP_NAME for emp in emps" name="EMP_ID" ng-model="select.EMP_ID" required></select></td>
				<td ng-show="travel.EMP_NAME.$touched && travel.EMP_NAME.$invalid">Select an employee.</td>
			</tr>
			<tr>
				<td>Vehicle: </td>
				<td><select ng-options="vehicle._id as vehicle.VEHICLE_NAME for vehicle in vehicles" name="VEHICLE" ng-model="select.VEHICLE_ID" required></select></td>
				<td ng-show="travel.EMP_NAME.$touched && travel.EMP_NAME.$invalid">Select a vehicle.</td>
			</tr>
			<tr>
				<td>Date: </td>
				<td><input type="date" name="DATE" ng-model="select.DATE" required></td>
				<td ng-show="travel.DATE.$touched && travel.DATE.$invalid">Enter a date.</td>
			</tr>
			<tr>
				<td>Start Time: </td>
				<td><input type="time" name="TIME_START" ng-model="select.TIME_START" required></td>
				<td ng-show="travel.TIME_START.$touched && travel.TIME_START.$invalid">Enter a time.</td>
			</tr>
			<tr>
				<td>Stop Time: </td>
				<td><input type="time" name="TIME_END" ng-model="select.TIME_END" required></td>
				<td ng-show="travel.TIME_END.$touched && travel.TIME_END.$invalid">Enter a time.</td>
			</tr>
		</tbody>
	</table>
    <input type="submit" value="Submit">
</form>
