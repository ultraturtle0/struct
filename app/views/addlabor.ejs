<form id="labor" ng-submit="laborSubmit()" method="post">
      <table>
            <tbody>
                  <tr>
                        <td>Job Number: </td>
                        <td><select ng-options="job._id as job.JOB_NUMBER for job in jobs" ng-model="select.JOB_ID"></select></td>
                  </tr>
                  <tr>
                        <td>Employee: </td>
                        <td><select ng-options="emp._id as emp.EMP_NAME for emp in emps" ng-model="select.EMP_ID"></select></td>
                  </tr>
                  <tr>
                        <td>Date: </td>
                        <td><input type="date" name="DATE" ng-model="select.DATE"></td>
                  </tr>
                  <tr>
                        <td>Start Time: </td>
                        <td><input type="time" name="TIME_START" ng-model="select.TIME_START"></td>
                  </tr>
                  <tr>
                        <td>Stop Time: </td>
                        <td><input type="time" name="TIME_END" ng-model="select.TIME_END"></td>
                  </tr>
                  <tr>
                        <td>Trade: </td>
                        <td><select ng-options="trade as trade.TRADE_NAME for trade in trades" ng-model="focustrade"></select></td>
                  </tr>
                  <tr>
                        <td>Work: </td>
                        <td><select ng-options="work._id as work.WORK_NAME for work in focustrade.WORKS" ng-model="select.SUBCATEGORY"></select></td>
                  </tr>
                  <tr>
                        <td>Description: </td>
                        <td><input type="text" name="DESCRIPTION" ng-model="select.DESCRIPTION"></td>
                  </tr>
            </tbody>
      </table>
      <input type="submit" value="Submit">
</form>