<!DOCTYPE html>
<html>
    <head>
        <title>Jobs - Sound Sport Surfaces</title>
        <script type="text/javascript" src="/lib/angular/angular.min.js">
        <script type="text/javascript" src="/lib/angular-resource/angular-resource.min.js">
    </head>
    <body ng-app="Database" ng-controller="JobController">
        <table>
            <tr ng-repeat="job in jobs">
                <td>{{ job.JOB_NUMBER }}</td>
                <td>{{ job.JOB_NAME }}</td>
                <td>{{ job.CUSTOMER_NAME }}</td>
                <td>{{ job.CUSTOMER_PHONE }}</td>
                <td>{{ job.CUSTOMER_EMAIL }}</td>
            </tr>
        </table>
        <script type="text/javascript" src="/public/js/controllers/jobs.client.controllers.js">
        <script type="text/javascript" src="/public/js/services/job.service.js">
    </body>
</html>

